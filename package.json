{
  "name": "markdown-doctest",
  "version": "1.0.0",
  "description": "Test all the code in your markdown docs!",
  "main": "dist/doctest.js",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "markdown-doctest": "bin/cmd.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Widdershin/markdown-doctest.git"
  },
  "keywords": [
    "markdown",
    "test",
    "documentation"
  ],
  "author": "Nick Johnstone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Widdershin/markdown-doctest/issues"
  },
  "homepage": "https://github.com/Widdershin/markdown-doctest",
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.7.12",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "chalk": "^2.4.2",
    "commander": "^4.0.1",
    "glob": "^7.1.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  }
}
